<%= @school.name %>
  <% if @school.members.any? %>
  <% @school.members.each do |member| %>
    <div class="member-container">
      <span class="member-name"><%=member.name%></span>
      <span class="member-email"><%=member.email%></span>
    </div>
  <% end %>
<% else %>
  No Attendees yet
<% end %>
